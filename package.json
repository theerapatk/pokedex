{
  "name": "pokedex",
  "version": "1.0.0",
  "author": "Jojo Theerapat",
  "description": "The Pokedex App",
  "scripts": {
    "ng": "ng",
    "build": "ng build && tsc -p pokedex-server",
    "buildprod": "ng build --aot --prod && tsc -p pokedex-server",
    "start": "node dist/pokedex-server/app.js",
    "predev": "tsc -p pokedex-server",
    "dev": "concurrently \"mongod\" \"ng serve --open\" \"tsc -w -p pokedex-server\" \"nodemon dist/pokedex-server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && tsc -p pokedex-server && node dist/pokedex-server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p pokedex-server && mocha dist/pokedex-server/test --exit",
    "lint": "ng lint && htmlhint \"pokedex-client/**/*.html\" && sass-lint \"pokedex-client/**/*.scss\" -v && tslint pokedex-server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "tslint": "tslint --project tsconfig.json"
  },
  "engines": {
    "node": "14.15.4",
    "npm": "6.14.10"
  },
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@hapi/joi": "^17.1.1",
    "aws-sdk": "^2.854.0",
    "bcrypt": "^5.0.0",
    "chart.js": "^2.9.4",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-jwt": "^6.0.0",
    "express-jwt-permissions": "^1.3.3",
    "http-errors": "^1.8.0",
    "http-server": "^0.12.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ng2-charts": "^2.4.2",
    "ngx-facebook": "^3.0.0-0",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-toastr": "^13.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "~11.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/http-errors": "^1.8.0",
    "@types/jasmine": "~3.6.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "htmlhint": "^0.14.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.7",
    "protractor": "~7.0.0",
    "sass-lint": "^1.13.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theerapatk/pokedex.git"
  },
